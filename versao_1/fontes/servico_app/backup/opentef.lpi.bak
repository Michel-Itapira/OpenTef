      </Unit>
      <Unit>
        <Filename Value="opentefnucleo.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="DNucleo"/>
        <ResourceBaseClass Value="DataModule"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <PathDelim Value="\"/>
    <Target>
      <Filename Value="..\..\binarios\servico_app\opentef"/>
